{
  "root": true,
  "extends": [
    "exp/all",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/extensions": 0
  },
  "settings": {
    "import/resolver": { "node": { "extensions": [".ts", ".js"] } }
  }
}
